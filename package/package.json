{
  "name": "@aliemir/dom-to-fiber-utils",
  "description": "A simple set of utilities to convert DOM elements to React Fiber elements",
  "private": false,
  "version": "0.5.0",
  "author": {
    "name": "Ali Emir Åžen",
    "email": "senaliemir@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/aliemir/dom-to-fiber-utils.git",
    "directory": "package"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "types": "node ../shared/generate-declarations.js"
  },
  "dependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-reconciler": "^0.29.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-reconciler": "^0.28.4",
    "typescript": "^5.0.2",
    "tsup": "^7.2.0"
  }
}
